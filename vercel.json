{
	"headers": [
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "Content-Security-Policy",
					"value": "default-src 'self' encryptedlist.vercel.app  api.airtable.com; img-src *.imagekit.io cdn.ko-fi.com; font-src fontlist.vercel.app"
				},
				{
					"key": "X-Content-Type-Options",
					"value": "nosniff"
				},
				{
					"key": "X-Frame-Options",
					"value": "DENY"
				},
				{
					"key": "X-XSS-Protection",
					"value": "1; mode=block"
				},
				{
					"key": "Referrer-Policy",
					"value": "strict-origin"
				},
				{
					"key": "Permissions-Policy",
					"value": "geolocation=(self), microphone=()"
				}
			]
		}
	]
}
